<UserControl x:Class="pdfforge.PDFCreator.UI.Presentation.UserControls.Settings.General.GeneralSettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:regions="http://prismlibrary.com/"
             mc:Ignorable="d"
             xmlns:redesign="clr-namespace:pdfforge.PDFCreator.UI.Presentation"
             xmlns:presentation="clr-namespace:pdfforge.PDFCreator.UI.Presentation"
             xmlns:general="clr-namespace:pdfforge.PDFCreator.UI.Presentation.UserControls.Settings.General"
             d:DataContext="{d:DesignInstance general:GeneralSettingsViewModel}"
             d:DesignHeight="350" d:DesignWidth="300">
    <Grid>
        <ItemsControl
            regions:RegionManager.RegionName="{x:Static presentation:RegionNames.GeneralSettingsTabContentRegion }" />
    </Grid>
</UserControl>