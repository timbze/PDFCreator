<UserControl x:Class="pdfforge.PDFCreator.UI.Presentation.UserControls.Settings.DebugSettings.DebugSettingView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:regions="http://prismlibrary.com/"
             xmlns:redesign="clr-namespace:pdfforge.PDFCreator.UI.Presentation"
             xmlns:designTime="clr-namespace:pdfforge.PDFCreator.UI.Presentation.DesignTime"
             xmlns:presentation="clr-namespace:pdfforge.PDFCreator.UI.Presentation"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance designTime:DesignTimeDebugSettingsViewModel, IsDesignTimeCreatable=True}"
             d:DesignHeight="300" d:DesignWidth="300">

    <Grid Margin="50 5 50 0">
        <ItemsControl
            regions:RegionManager.RegionName="{x:Static presentation:RegionNames.DebugSettingsTabContentRegion}"/>

    </Grid>
</UserControl>